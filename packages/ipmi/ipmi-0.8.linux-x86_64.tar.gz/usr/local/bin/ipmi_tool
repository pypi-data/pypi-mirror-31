#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ipmi==0.8','console_scripts','ipmi_tool'
__requires__ = 'ipmi==0.8'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ipmi==0.8', 'console_scripts', 'ipmi_tool')()
    )
